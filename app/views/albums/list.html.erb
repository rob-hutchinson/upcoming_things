<table class="table table-striped table-hover ">
  <thead>
    <tr>
      <th>#</th>
      <th>Artist</th>
      <th>Album</th>
      <th>Release Date</th>
    </tr>
  </thead>
  <tbody>
    <% @albums.each do |x| %>
      <tr>
        <td><%= x.id %></td>
        <td><%= x.artist %></td>
        <td><%= x.album_title %></td>
        <td><%= x.release_date %></td>
      </tr>
    <% end %>
</table> 