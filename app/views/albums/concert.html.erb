<table class="table table-striped table-hover ">
  <thead>
    <tr>
      <th></th>
      <th>Artist</th>
      <th>Venue</th>
      <th>Concert Date</th>
      <th>Tickets</th>
      <th>
    </tr>
  </thead>
  <tbody>
    <% @concerts.each do |x| %>
      <tr>
        <td><img src= <%= x["image"]["medium"]["url"]%>></td>
        <td><%= x["title"] %></td>
        <td><%= x["venue_name"] %></td>
        <td><%= Date.parse(x["start_time"]) %></td>
        <td><%= link_to "Search For Tickets", x["url"] %></td>
      </tr>
    <% end %>
</table> 